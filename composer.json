{
    "name": "mo3200taha/archiv_php",
    "type": "project",
    "description": "Electronic Document Archiving System",
    "keywords": ["laravel", "document", "archiving"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
